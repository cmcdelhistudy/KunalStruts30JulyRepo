<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
          "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="default" extends="struts-default">
		<action name="rat" class="com.cmcdelhi.action.MyAction">
			<result name="success">/success.jsp</result>
			<result name="error">/failure.jsp</result>
			<result name="katrina">/kat.jsp</result>			
		</action>
		
		<action name="showStats" class="com.cmcdelhi.action.ChartAction">
			<result name="success">/jqstudy.jsp</result>
			<result name="error">/failure.jsp</result>
			<result name="katrina">/kat.jsp</result>			
		</action>
		
		
		<action name="vstudy" class="com.cmcdelhi.action.ValidationStudyAction">
			<result name="success">/successv.jsp</result>
			<result name="input">/index.jsp</result>
			<result name="error">/failure.jsp</result>
						
		</action>
	</package>

</struts>
